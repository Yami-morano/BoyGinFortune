@model List<BoyGin.Models.DTOMateriaPrima>
@{
    ViewBag.Title = "MateriasprimasStock";
}


<h2>Listado de materias primas en stock</h2>

<table class="table table-bordered table-striped table-hover">
    <tr>

        <th>Descripcion</th>
    
        <th>Cantidad</th>

    </tr>

    @foreach (var item in Model)
    {

        <tr>

            <td>@item.descripcionMateriaPrima</td>
           
            <td>@item.stockMateriaPrima</td>
           
        </tr>
    }

</table>

